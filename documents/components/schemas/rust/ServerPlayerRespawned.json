{
	"$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
	"$id": "ServerPlayerRespawned",
  "required": [
    "steam_id",
    "timestamp",
    "position"
  ],
  "properties": {
    "steam_id": {
      "type": "string",
      "description": "The player's steamID64, must be 17 chars long.",
      "pattern": "^[0-9]{17}$"
    },
    "timestamp": {
      "type": "string",
      "example": "2016-08-29T09:12:33.001Z"
    },
    "position": {
      "$ref": "../generic/Vector3d.json"
    }
  }
}