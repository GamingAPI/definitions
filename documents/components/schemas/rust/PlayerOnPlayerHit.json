{
	"$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
	"$id": "PlayerOnPlayerHit",
  "required": [
    "hit_area_id",
    "hit_distance",
    "hit_damage",
    "is_kill",
    "victim",
    "attacker",
    "timestamp"
  ],
  "properties": {
    "hit_area_id": {
      "type": "integer"
    },
    "hit_distance": {
      "type": "number"
    },
    "hit_damage": {
      "type": "number"
    },
    "is_kill": {
      "type": "boolean"
    },
    "victim": {
      "$ref": "./PlayerHit.json"
    },
    "attacker": {
      "$ref": "./PlayerHit.json"
    },
		"timestamp": {
			"type": "string",
			"example": "2016-08-29T09:12:33.001Z"
		}
  }
}